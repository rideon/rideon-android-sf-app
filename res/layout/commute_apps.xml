<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/commuter_app_layout_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".WheelsMainActivity" >

    <RelativeLayout
        android:id="@+id/commuter_app_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/roundbg"
        android:gravity="top|center_horizontal|center"
        android:orientation="horizontal"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp" >

        <!-- android:layout_below="@+id/button_wheels" -->

        <Button
            android:id="@+id/button_actransit"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="acTransit"
            android:padding="0dip"
            android:text="@string/actransit_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_bart"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_actransit"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="bart"
            android:padding="0dip"
            android:text="@string/bart_button_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_caltrain_realtime"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_bart"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="caltrainRealtime"
            android:padding="0dip"
            android:text="@string/bart_caltrain_realtime"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_dumbartonexpress"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_actransit"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="dumbartonExpress"
            android:padding="0dip"
            android:text="@string/dumbartonexpress_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_samtrans"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_bart"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_dumbartonexpress"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="samTrans"
            android:padding="0dip"
            android:text="@string/samtrans_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_muni_stops"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_bart"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_samtrans"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="muniStops"
            android:padding="0dip"
            android:text="@string/muni_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <!--
             <Button
            android:id="@+id/button_caltrain_byroute"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_actransit"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_bart"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="gtfsRoute"
            android:padding="0dip"
            android:text="@string/bart_caltrain_route_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />
        -->

        <Button
            android:id="@+id/button_vta"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_dumbartonexpress"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="vta"
            android:padding="0dip"
            android:text="@string/vta_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_westcat"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_dumbartonexpress"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_vta"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="westcat"
            android:padding="0dip"
            android:text="@string/westcat_button_stops_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_wheels"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_dumbartonexpress"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_westcat"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="wheels"
            android:padding="0dip"
            android:text="@string/wheels_button_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/button_vta_rail"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_wheels"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="vtaRails"
            android:padding="0dip"
            android:text="@string/vta_rails_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />
        <Button
            android:id="@+id/button_caltrain_schedule"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_below="@+id/button_wheels"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/button_vta_rail"
            android:background="@drawable/roundbutton"
            android:enabled="true"
            android:gravity="center_vertical|center_horizontal|center"
            android:onClick="caltrain"
            android:padding="0dip"
            android:text="@string/bart_caltrain_stop_label"
            android:textColor="#eeeeee"
            android:textSize="15sp"
            android:textStyle="normal" />        
        
    </RelativeLayout>

</RelativeLayout>